    "express": "^5.0.0-beta.1",
    "express-validator": "^6.14.2",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.7",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.14",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.1.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.11.64",
    "@types/pg": "^8.6.1",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
