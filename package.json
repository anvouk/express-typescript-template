{
  "name": "express-typescript-template",
  "version": "0.1.0",
  "description": "Custom express typescript webservice",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --outdir=dist --bundle --minify --target=node20 --external:pg-native",
    "start": "node .",
    "start:dev": "ts-node-dev --rs --watch .env src/index.ts | pino-pretty",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.6.1",
    "esbuild": "^0.19.5",
    "jest": "^29.1.2",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^5.0.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^20",
    "npm": "^10"
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.2.3"
  }
}
