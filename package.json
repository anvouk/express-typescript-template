    "bunyan": "^1.8.15",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^5.0.0-beta.1",
    "express-validator": "^6.12.2",
    "helmet": "^5.0.2",
    "ioredis": "^5.0.4",
    "morgan": "^1.10.0",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.7",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^27.4.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^16.11.26",
    "@types/pg": "^8.6.1",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.6.2",
