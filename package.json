{
  "name": "express-typescript-template",
  "version": "0.1.0",
  "description": "Custom express typescript webservice",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "esbuild src/index.ts --platform=node --outdir=dist --bundle --minify --target=node18 --packages=external",
    "start": "node .",
    "start:dev": "ts-node-dev --rs --watch .env src/index.ts | pino-pretty",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^5.0.0-beta.1",
    "express-validator": "^7.0.1",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pino": "^8.8.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.1.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.1",
    "esbuild": "^0.17.17",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.4.0",
    "typescript": "^5.0.4"
  },
  "engineStrict": true,
  "engines": {
    "node": "^18",
    "npm": "^9"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19",
    "npm": "9.6.4"
  }
}
